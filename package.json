{
  "name": "mobile-inspections-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.0.2",
    "axios": "^0.19.2",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-split-pane": "^0.1.89",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rt-design": "git+https://github.com/air-tech/react-tech-design.git#semver:^0.0.10"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^2.23.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "http-proxy-middleware": "^1.0.1",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.4.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "test": "react-app-rewired test",
    "eject": "eact-app-rewired eject",
    "lint": "eslint \"**/*.js\" --max-warnings=0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss, less, md}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
